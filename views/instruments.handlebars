<section>
    <h3 class="heading">{{family_name}}</h3>
    <section class="flex-row justify-center">
        {{#each instruments}}
        <section class="card">
            <div class="card-body">
                <img src={{image}} alt={{instrument_name}}>
                <p class="bold">{{instrument_name}}</p>
                <p class="bold">{{price}}</p>
                <p>Sold by: {{user_id}}</p>
            </div>
        </section>
    </section>
</section>